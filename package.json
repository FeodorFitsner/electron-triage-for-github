  },
  "dependencies": {
    "electron-debug": "^0.1.1"
  },
  "devDependencies": {
    "conventional-changelog": "*",
    "david": "^6.1.6",
    "eclint": "^0.2.6",
    "electron-packager": "^5.0.1",
    "electron-prebuilt": "^0.30.1",
    "eslint": "^1.0.0-rc",
    "espower-loader": "^0.11.0",
    "fixpack": "^2.2.0",
    "globstar": "^1.0.0",
    "mocha": "^2.2.5",
    "power-assert": "^0.11.0",
    "touch": "^1.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "electron-app"
  ],
  "license": "MIT",
  "main": "",
  "repository": "sanemat/npm-modules",
  "scripts": {
    "changelog": "touch changelog.md && conventional-changelog -i changelog.md --overwrite --preset angular",
    "david": "david update",
    "eclint": "globstar --ignore \"node_modules/**\" -- eclint check \"*.{js,yml,md,json}\"",
    "eslint": "eslint .",
    "fixpack": "fixpack",
    "git:tag": "git tag v${npm_package_version}",
    "lint": "npm run eclint && npm run eslint",
    "start": "electron dist",
    "test": "mocha --require ./enable-power-assert.js -- ./test.js",
    "verify": "npm run lint && npm run test"
  }
}
